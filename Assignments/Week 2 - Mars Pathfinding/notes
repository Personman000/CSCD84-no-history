% Initialize queues
(node_index, cost_to_get_to_node_from_start, node_parent)
Open=[][][];
(node_index, node_parent)
Visited=[][];

% Start path at start
Path(1) = s;
Open(1) = s;

// Loop until goal found
while(~ismember(g, Visited))

	curr_node = Open(1, 1)
	
	// Loop through neighbors
	for i=1:4
		curr_neighbor = curr_node[i]
		
		// Put neighbors into Open
		if(curr_neighbor != -1 & curr_neighbor not in Open or Visited)
		
			Open.push(curr_neighbor_index, curr_node_index.weight + curr_neighbor.weight, curr_node)
			
			end
		end
	Visited.push(curr_node, curr_node_parent)
	Open.sortby(weight)
		
	end

curr_node =  Visited.getIndexOf(g)
while(~ismember(s, Path))
	Path(i) = curr_node
	curr_node = Visited(Visited.getIndexOf(curr_node), 2)
	end
```
```
classdef node
	properties
		index {mustBeNumeric}
		parent
		weight {museBeNumeric}
		end
	end


	
% Initialize queues
Open=[];
Visited=[];

% Start path at start
start_node = node;
start_node.index=s;
start_node.parent=-1
start_node.weight=0

Open(1) = start_node;

// Loop until goal found
while(~ismember(g, Visited))

	curr_node = Open(1)

	// Loop through neighbors
	for i=1:4
		curr_neighbor = Gid(curr_node.index, i)
		
		// Put neighbors into Open
		if(curr_neighbor != -1 & curr_neighbor not in Open or Visited)
			
			curr_neighbor.parent = curr_node
			curr_neighbor.weight = curr_node.weight + G(curr_node.index, i)
			Open.push(curr_neighbor)
			
			end
		end
		
	Visited.push(curr_node)
	Open.sortby(weight)
		
	end

while(curr_node.parent != -1)
	Path.push(curr_node.index)
	curr_node = curr_node.parent
	end
```
